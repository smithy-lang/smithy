              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/FooBarResponseContent"
                }
              }
            }
          },
          "400": {
            "description": "FooBarError 400 response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/FooBarErrorResponseContent"
                }
              }
            }
          }
        }
      }
    }
  },
