      "FooBarRequestContent": {
        "type": "object",
        "properties": {
          "foo": {
            "type": "number"
          },
          "file": {
            "type": "string",
            "format": "byte"
          }
        }
      },
      "FooBarResponseContent": {
        "type": "object",
        "properties": {
          "bar": {
            "type": "number"
          },
          "baz": {
            "$ref": "#/components/schemas/MyMap"
          }
        }
      },
      "MyMap": {
        "type": "object",
        "additionalProperties": {
          "type": "string"
        }
      }
    }
  }
