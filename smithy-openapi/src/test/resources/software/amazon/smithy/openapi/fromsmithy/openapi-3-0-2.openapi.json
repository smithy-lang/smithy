{
  "openapi": "3.0.2",
  "info": {
    "title": "Example",
    "version": "2022-07-10"
  },
  "paths": {
    "/test": {
      "put": {
        "operationId": "FooBar",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/FooBarRequestContent"
              }
            }
          }
        },
