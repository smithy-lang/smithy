{
    "version": "1.0",
    "projections": {
        "plain-markdown": {
            "plugins": {
                "docgen": {
                    "service": "com.example#DocumentedService",
                    "format": "markdown",
                    "references": {
                        "com.example#ExternalResource": "https://aws.amazon.com"
                    }
                }
            }
        },
        "sphinx-markdown": {
            "plugins": {
                "docgen": {
                    "service": "com.example#DocumentedService",
                    "format": "sphinx-markdown",
                    "references": {
                        "com.example#ExternalResource": "https://aws.amazon.com"
                    }
                }
            }
        }
    }
}
